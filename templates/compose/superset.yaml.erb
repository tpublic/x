version: '3.6'

services:
  superset:
    image: "<%= @utils['images']['superset'] %>"
    restart: always
    volumes:
      - superset_db:/var/lib/superset
    labels:
      traefik.enable: true
      traefik.frontend.rule: "Host:superset.<%= @config['app']['domain'] %>"
      traefik.port: 8088

volumes:
  superset_db:
